[sub_resource type="GDScript" id="GDScript_fce3b"]
script/source = "@tool
extends Node3D


const SPINNY_BIT_FLAGS: PackedByteArray = [
	0b1100000,
	0b0110000,
	0b0011000,
	0b0001100,
	0b0000110,
	0b1000010
]

@onready var spinny_display: SevenSegmentDisplay = $spinny_display
@onready var hours_display: SevenSegmentDisplay = $hours_display
@onready var minutes_display: SevenSegmentDisplay = $minutes_display
@onready var seconds_display: SevenSegmentDisplay = $seconds_display


func _notification(what: int) -> void:

	if what == NOTIFICATION_EDITOR_PRE_SAVE:
		spinny_display.content_bit_flags[0] = 0
		hours_display.content_integer = 0
		minutes_display.content_integer = 0
		seconds_display.content_integer = 0


func _process(delta: float) -> void:

	var spinny_bf := SPINNY_BIT_FLAGS[floori(Time.get_ticks_msec() / 100.0) % SPINNY_BIT_FLAGS.size()]
	if spinny_bf != spinny_display.content_bit_flags[0]:
		spinny_display.content_bit_flags = [spinny_bf]

	var t := Time.get_datetime_dict_from_system()
	hours_display.content_integer = t[\"hour\"]
	minutes_display.content_integer = t[\"minute\"]
	seconds_display.content_integer = t[\"second\"]
"

